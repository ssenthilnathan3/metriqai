# API Configuration
VITE_API_BASE_URL=http://localhost:8000

# Application Configuration
VITE_APP_TITLE="ML Benchmark Dashboard"
VITE_APP_DESCRIPTION="Comprehensive analysis of machine learning model performance"

# Feature Flags
VITE_ENABLE_DEV_TOOLS=true
VITE_ENABLE_ANALYTICS=false

# Chart Configuration
VITE_DEFAULT_CHART_HEIGHT=500
VITE_MAX_CHART_POINTS=1000

# Data Refresh Configuration
VITE_AUTO_REFRESH_INTERVAL=300000
VITE_CACHE_TTL=600000

# Performance Configuration
VITE_LAZY_LOAD_CHARTS=true
VITE_ENABLE_SERVICE_WORKER=false

# Theme Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_THEME_TOGGLE=true

# Debug Configuration
VITE_LOG_LEVEL=info
VITE_ENABLE_PERFORMANCE_MONITORING=false
